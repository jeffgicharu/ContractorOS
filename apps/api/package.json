{
  "name": "@contractor-os/api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "nest start --watch",
    "build": "nest build",
    "start": "node dist/main",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "jest",
    "test:integration": "jest --config test/jest.config.ts",
    "migrate:up": "node --import tsx node_modules/node-pg-migrate/bin/node-pg-migrate.js up --migrations-dir src/database/migrations",
    "migrate:down": "node --import tsx node_modules/node-pg-migrate/bin/node-pg-migrate.js down --migrations-dir src/database/migrations",
    "migrate:create": "node-pg-migrate create --migrations-dir src/database/migrations --tsconfig tsconfig.json",
    "seed": "tsx src/database/seeds/seed.ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@contractor-os/shared": "workspace:*",
    "@nestjs/common": "^11.1.13",
    "@nestjs/core": "^11.1.13",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.13",
    "@nestjs/schedule": "^6.1.1",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.3.1",
    "node-pg-migrate": "^8.0.4",
    "tsx": "^4.21.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.18.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.16",
    "@nestjs/testing": "^11.1.13",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.0.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
